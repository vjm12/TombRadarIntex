@{
}

@model IEnumerable<string>

<div class="d-grid gap-2 bg-light">
    <a asp-controller="Home" asp-action="Summary" asp-route-burialsex="" asp-route-pageNum ="1" class="btn btn-outline-secondary">All</a>
    @foreach (string t in Model)
    {
    <a asp-controller="Home" asp-action="Summary" asp-route-burialsex="@t" asp-route-pageNum ="1"
       class="btn @(ViewBag.SelectedSex == t ? "btn-primary" :"btn-outline-secondary")" >@t</a>
    }
</div>

