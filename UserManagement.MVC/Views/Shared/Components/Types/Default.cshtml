@{
}

@model IEnumerable<string>

<div class="d-grid gap-2 bg-light">
    <p>Square North-South Filter</p>
    <a asp-controller="Home" asp-action="Summary" asp-route-burialdirec="" asp-route-pageNum="1" class="btn btn-outline-secondary">All</a>
    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Summary" asp-route-burialdirec="@t" asp-route-pageNum="1"
           class="btn @(ViewBag.SelectedDirec == t ? "btn-primary" :"btn-outline-secondary")">@t</a>
    }
</div>

